{
  "name": "vue3-vite-typescript-starter",
  "version": "0.0.1",
  "private": false,
  "description": "Simple starter template for developing Vue 3 app with some often used features",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "eslint --ext .ts,vue --ignore-path .gitignore . --fix",
    "format": "prettier -w -u .",
    "e2e:install": "cypress install",
    "e2e:run": "cypress run --headless",
    "e2e:run-ct": "cypress run-ct --headless",
    "e2e:open": "cypress open",
    "e2e:open-ct": "cypress open-ct",
    "e2e:test": "yarn e2e:run && yarn e2e:run-ct",
    "deploy": "./deploy.sh"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.15",
    "@quasar/vite-plugin": "^1.9.0",
    "axios": "1.7.9",
    "gh-pages": "^6.3.0",
    "inversify": "^6.2.1",
    "quasar": "^2.17.6",
    "reflect-metadata": "^0.2.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^6.0.1",
    "@cypress/vue": "^6.0.2",
    "@types/node": "^22.10.7",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "cypress": "^14.0.0",
    "eslint": "^9.18.0",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-vue": "^9.32.0",
    "prettier": "^3.4.2",
    "sass": "^1.83.4",
    "typescript": "^5.7.3",
    "vite": "6.0.7",
    "vue-tsc": "^2.2.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
